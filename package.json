  "author": "Daniel Verdejo",
  "license": "UNLICENSED",
  "dependencies": {
    "@types/config": "0.0.34",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/mongoose": "^5.3.12",
    "config": "^3.0.0",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "jwks-rsa": "^1.3.0",
    "mongoose": "^5.4.11",
    "ts-node": "^7.0.1",
    "tsc-init": "^2.1.0",
    "typescript": "^3.2.1"
  }
}
